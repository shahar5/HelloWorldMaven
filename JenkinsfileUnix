pipeline { 
    agent any 
    stages {
        stage('Build') { 
            steps {
                withMaven(maven : 'apache-maven-3.3.3', mavenSettingsConfig: 'settings.xml'){
                        sh "mvn clean compile"
                }
            }
        }
        stage('Test'){
            steps {
                withMaven(maven : 'apache-maven-3.3.3'){
                        sh "mvn test"
                }

            }
        }
    }
}
